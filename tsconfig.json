{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": { "*": ["types/*"] },
		"outDir": "./built",
		"target": "es2020",
		"module": "commonjs",
		"noImplicitAny": true,
		"lib": ["es2019"],
		"resolveJsonModule": true,
		"declaration": false,
		"strict": true,
		"esModuleInterop": true,
		"inlineSourceMap": true
	},
	"include": ["src/**/*", "assets/**/*", "index.ts"]
}
